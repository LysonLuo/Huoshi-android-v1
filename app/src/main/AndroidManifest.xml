<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="im.huoshi">

    <application
        android:name=".HuoshiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.main.LoginActivity" />
        <activity android:name=".ui.find.SuffrageActivity" />
        <activity android:name=".ui.find.FiveTwoActivity" />
        <activity android:name=".ui.find.SuffrageDetailsActivity" />
        <activity android:name=".ui.find.ShareHuoshiActivity" />
        <activity android:name=".ui.me.MyPrayerActivity" />
        <activity android:name=".ui.me.UserInfoActivity" />
        <activity android:name=".ui.find.PubSuffrageActivity" />
        <activity android:name=".ui.bible.ChapterDetailsActivity" />
        <activity android:name=".ui.main.RegisterActivity" />
        <activity android:name=".ui.main.RegisterSettingActivity" />
    </application>

</manifest>
